{
  "name": "bun-proxy",
  "description": "M3u8 proxy made using pure NodeJS and Bun.",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/express-http-proxy": "^1.6.5",
    "bun-types": "^1.0.7",
    "prettier": "^3.0.3",
    "tsc": "^2.0.4"
  },
  "scripts": {
    "start": "bun run dist/index.js",
    "dev": "bun run index.ts",
    "build": "bun build index.ts --outdir ./dist --target node",
    "prettier": "bunx prettier --write .",
    "lint": "bun run prettier && bunx tsc"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0"
  }
}